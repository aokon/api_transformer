<%= form_for @conn, page_path(@conn, :index), [as: :search, method: "get"], fn f -> %>
  <div class="form-group">
    <%= text_input f, :url, class: "form-control", placeholder: "https://examle.com" %>
  </div>

  <div class="form-group">
    <%= select f, :type, ["json", "xml"], class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit "Send", class: "btn btn-primary" %>
  </div>
<% end %>
